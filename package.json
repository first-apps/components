  "description": "Components library for @first-apps built on React JS",
  "main": "dist/cjs/index.js",
  "module": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/first-apps/mf-components.git"
  },
  "author": {
    "name": "dikshit-n",
    "email": "dikshitkumarn@gmail.com"
  },
  "keywords": [
    "react",
    "components",
    "firstApps",
    "mf",
    "module federation",
